#include <stdio.h>
int main() 
{
   int array[14] = {1, 2, 3, 4, 5, 6, 7, 8, 9, 7,6,4,9,3};
   int sum, i;
   float avg;
    sum = avg = 0;
   for(i = 0; i < 14; i++)
    {
      sum = sum + array[i];
   }
   
   avg = (float)sum / i;
   printf("Average of array values is %.2f", avg);   
   
   return 0;
}
