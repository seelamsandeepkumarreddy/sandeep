#include<stdio.h>
int main()
{  
   int a[10][10],transpose[10][10],n,i,j;
   printf("enter the value of N(N<=10):");
   scanf("%d",&n);
   printf("enter the elements of matrix A:\n");
   for(i=0;i<n;i++)
   {
   	for(j=0;j<n;j++)
   	{
   		scanf("%d",&a[i][j]);
	   }
   }
  
	  for(i=0;i<n;i++)
	  {
	  	for(j=0;j<n;j++)
	  	{
	  		transpose [i][j]=a[j][i];
	  		printf("%d\t",transpose[i][j]);
	    }
	    printf("\n");
    }
		  return 0;
	  
	
      }
  
