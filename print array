#include <stdio.h>
int main()
 {
   int num[10],i,n;
   printf("enter the numbers:");
   scanf("%d",&n);

   for(i = 0; i < n; i++)
      {
      		printf("enter number %d:" ,i+1);
   	            scanf("%d",&num[i]);
	  }
	  for(i=0;i<n;i++)
	  {
	  	printf("%d\n",num[i]);
	  }
      
   return 0;
}
