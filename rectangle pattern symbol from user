#include<stdio.h>
int main()
{
	int rows,columns,i,j;
	char x;
	printf("enter the symbol from user");
	scanf("%c",&x);
	printf("enter the number of rows and columns:");
	scanf("%d%d",&rows,&columns);
	for(i=0;i<rows;i++)
	{
		for(j=0;j<columns;j++)
		{
			printf("%c",x);
		}
		printf("\n");
	}
	return 0;
}
