#include<stdio.h>
int main()
{
	int num[100],i,n,smallest;
	printf("enter the number of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("enter number %d:",i+1);
		scanf("%d",&num[i]);
	}
	if(smallest>num[i])
	smallest=num[i];
	{
		printf("smallest element of array is %d",smallest);
	}
}
